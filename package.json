{
  "name": "raspinodemon",
  "version": "0.0.0",
  "description": "tool to monitor raspinode",
  "main": "server.js",
  "scripts": {
    "test": "mocha --recursive tests/",
    "watch": "watchify js/app.js --poll=1000 -o static/bundle.js -vvv",
    "build": "browserify js/app.js | uglifyjs -mc warnings=false > static/bundle.js",
    "start": "nodemon --watch server.js --watch js/api"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KanoczTomas/raspinodemon.git"
  },
  "keywords": [
    "raspberry",
    "nodejs",
    "rest",
    "bootstrap"
  ],
  "author": "Tomas Kanocz",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/KanoczTomas/raspinodemon/issues"
  },
  "dependencies": {
    "angular": "^1.6.1",
    "angular-socket-io": "^0.7.0",
    "angular-ui-router": "^0.4.2",
    "bitcoinjs-lib": "^2.3.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "cache-require-paths": "^0.3.0",
    "compression": "^1.6.2",
    "config": "^1.24.0",
    "express": "^4.14.0",
    "get-folder-size": "^1.0.0",
    "js-yaml": "^3.7.0",
    "node-bitcoin-rpc": "^1.1.2",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "uglify-js": "^2.7.5",
    "underscore": "^1.8.3",
    "zeromq": "^4.1.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.1",
    "angular-mocks-node": "^1.3.2",
    "angular-node": "^1.3.2",
    "browserify": "^13.3.0",
    "config-browserify": "^1.0.5",
    "debowerify": "^1.5.0",
    "jsdom": "^3.1.2",
    "karma": "^1.4.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.2.0",
    "morgan": "^1.7.0",
    "proxyquire": "^1.7.11",
    "should": "^11.1.2",
    "sinon": "^1.17.7",
    "supertest": "^2.0.1",
    "watchify": "^3.8.0"
  },
  "browserify": {
    "transform": [
      "debowerify",
      "config-browserify"
    ]
  }
}
